{
  "workbench.startupEditor": "newUntitledFile",
  "git.autofetch": true,
  "git.confirmSync": false,
  "gitlens.keymap": "chorded",
  "workbench.editor.enablePreview": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false
  },
  "local-history.treeLocation": "localHistory",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "emmet.triggerExpansionOnTab": true,
  "files.autoSave": "afterDelay",
  "workbench.colorTheme": "Darcula",
  "editor.occurrencesHighlight": "off",
  "editor.selectionHighlight": false,
  "editor.renderWhitespace": "all",
  "editor.suggestSelection": "first",
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.iconTheme": "material-icon-theme",
  "todohighlight.isEnable": false,
  "files.insertFinalNewline": true,
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "git.enableSmartCommit": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.preferences.quoteStyle": "single",
  "[yaml]": {
    "editor.autoIndent": "advanced"
  },
  "editor.accessibilitySupport": "off",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1",
    "PYTHONPATH": "${workspaceFolder}"
  },
  "editor.stickyScroll.enabled": true,
  "workbench.colorCustomizations": {
    "editorStickyScroll.background": "#5a4040",
    "editorStickyScrollHover.background": "#170909"
  },
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "astro"
  ],
  "markdown.extension.toc.updateOnSave": false,
  "editor.fontFamily": "'JetBrains Mono', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 12.5,
  "editor.foldingImportsByDefault": true,
  "editor.linkedEditing": true,
  "editor.largeFileOptimizations": false,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.scrollback": 100000,
  "terminal.integrated.env.linux": {
    "PYTHONPATH": "${workspaceFolder}"
  },
  "files.trimTrailingWhitespace": true,
  "scm.alwaysShowRepositories": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "eslint.useFlatConfig": true,
  "eslint.options": {
    "overrideConfigFile": "/Users/sergiuminorescu/amphora-logistics-code-standards/configs/eslint/eslint.config.js"
  },
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "astro"
  ],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "eslint.format.enable": true,
  "editor.tabSize": 3,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "typescript.preferences.importModuleSpecifier": "relative",
  "files.associations": {
    "*.tsx": "typescriptreact",
    "*.css": "postcss",
    "*.vue": "vue",
    "pyproject.toml": "toml",
    ".flake8": "ini"
  },
  "emmet.includeLanguages": {
    "vue-html": "html",
    "vue": "html",
    "typescript": "typescriptreact",
    "javascript": "javascriptreact",
    "erb": "html",
    "astro": "html"
  },
  "python.linting.enabled": true,
  "python.linting.lintOnSave": true,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": "/Users/sergiuminorescu/amphora-logistics-code-standards/configs/python/.flake8",
  "python.formatting.provider": "black",
  "python.formatting.blackArgs": "/Users/sergiuminorescu/amphora-logistics-code-standards/configs/python/pyproject.toml",
  "python.sortImports.args": "/Users/sergiuminorescu/amphora-logistics-code-standards/configs/python/pyproject.toml",
  "[python]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  }
}
